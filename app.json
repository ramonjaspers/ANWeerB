{
  "expo": {
    "name": "ANWeerB",
    "slug": "ANWeerB",
    "version": "1.0.0",
    "orientation": "default",
    "userInterfaceStyle": "light",
    "scheme": "anweerb",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffcd00"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "This app needs access to your location to show weather information for your current location."
      },
      "bundleIdentifier": "com.ramonjaspers.ANWeerB",
      "icon": {
        "dark":"./assets/icon.png",
        "light": "./assets/icon.png",
        "tinted": "./assets/icon.png"  
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon/foreground.png",
        "backgroundImage": "./assets/adaptive-icon/background.png"
      },
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION"
      ],
      "package": "com.ramonjaspers.ANWeerB"
    },
    "plugins": [
      "expo-router",
      ["expo-screen-orientation",
        {
          "orientationLock": "ALL"
        }
      ]
    ]
  }
}
